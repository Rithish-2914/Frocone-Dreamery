{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "server/index.ts"
    },
    {
      "src": "/assets/(.*)",
      "dest": "client/dist/assets/$1",
      "continue": true
    },
    {
      "src": "/(.*)",
      "dest": "server/index.ts"
    }
  ]
}
